- var title = "Swift TO";
- var description = "Swift TO Presents a workshop with Paul Hudson in Downtown Toronto";
- var date = "August 13th 2020, 9:30-4:30pm EST"
- var venueName = "Remote Event"
- var venueUrl = ""
- var rfpUrl = "https://www.papercall.io/swift-to-2020" 

- var keywords = ["conference", "swift", "ios", "macos", "watchos", "linux", "server-side", "apple"].join(",")

- var twitter1 = "SwiftConfTO"
- var twitter2 = "NSCoderTO"
- var email = "hello@swiftconf.to"
- var companyName = "NSCoderTO and Swift TO Inc., a non-profit corporation"

- var expectation1 = {title: "Learn from Paul&nbsp;Hudson", description: "An all day workshop featuring Paul Hudson from <a href='https://www.hackingwithswift.com' target='_blank'>HackingWithSwift.com</a>"};
- var expectation2 = {title: "Live from Toronto in EST", description: "Featuring highlights from <a href='https://www.hackingwithswift.com/live' target='_blank'>Hacking With Swift Live</a>, this workshop will be held in the EST Timezone."};
- var expectation3 = {title: "100% Remote", description: "This event will be run remotely so you can enjoy a day of coding with new friends from the comfort of home."};
- var expectations = [expectation1, expectation2, expectation3];

- var speaker1 = {name: "Paul Hudson", slug: "paul-hudson", twitter: "twostraws"};
- var speakers = [speaker1];

doctype html
html(lang="en")
  head
    title=title
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    meta(name="description", content=description)
    meta(name="keywords", content=keywords)
    link(href="https://fonts.googleapis.com/css?family=Playfair+Display:400,700,900|Raleway", rel="stylesheet")
    link(rel="stylesheet", href="css/bootstrap.css")
    link(rel="stylesheet", href="css/animate.css")
    link(rel="stylesheet", href="fonts/ionicons/css/ionicons.min.css")
    link(rel="stylesheet", href="fonts/fontawesome/css/font-awesome.min.css")
    // Twitter card
    meta(name="twitter:card", content="summary")
    meta(name="twitter:site", content="@" + twitter1)
    meta(name="twitter:title", content=title)
    meta(name="twitter:description", content=description)
    meta(name='twitter:image', content='https://swiftconf.to/images/swift-to-logo.png')

    //Open Graph
    meta(property="og:title", content=title)
    meta(property="og:image", content="https://swiftconf.to/images/swift-to-logo.png")
    meta(property="og:description", content=description)
    meta(property="og:url", content="https://swiftconf.to")

    // Theme Style
    link(rel="stylesheet", href="css/style.css")

  body
    header(role="banner")
      nav#nav-app.navbar.navbar-expand-md.navbar-dark.bg-light
        .container
          a.navbar-brand(href='index.html')=title
          a.navbar-brand.small(href='index.html')=title
          button.navbar-toggler(type='button')
            span.navbar-toggler-icon
          div.navbar-collapse.navbar-light.collapse
            ul.navbar-nav.ml-auto.pl-lg-5.pl-0
              li.nav-item
                a.nav-link(href='/#tickets') Tickets

    div#app
      block content

    footer.site-footer
      .container
        .row.mb-5
          .col-md-4.mb-2
            h3.mb-4 About
            p.mb-4
              | #{description}
          .col-md-4.mb-2
            h3.mb-4 Links
            ul.list-unstyled
              li
                a(href='/') Home
              li
                a(href='/#tickets') Tickets
              li
                a(href='/sponsorship.html') Sponsorship
              li
                a(href='https://2019.swiftconf.to') 2019 Website
          .col-md-4.mb-2
            h3.mb-4 Contact
            p
              a(target='_blank', href='https://twitter.com/' + twitter1)
                span.fa.fa-twitter
                | &nbsp;
                span=twitter1
            p
              a(target='_blank', href='https://twitter.com/' + twitter2)
                span.fa.fa-twitter
                | &nbsp;
                span=twitter2
            p
              a(target='_blank', href='mailto:' + email)
                span=email
            p
              span Organized by #{companyName}

    script(src='js/main.js')